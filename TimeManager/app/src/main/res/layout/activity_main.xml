<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.master.timemanager.MainActivity">

    <Button
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:id="@+id/btnGroup"
            android:text="群组" tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="105dp"/>
    <Button
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:id="@+id/btnCalendar"
            android:text="日历" tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="193dp"/>

    <WebView
            android:id="@+id/wv_webview"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="9" />


</android.support.constraint.ConstraintLayout>
